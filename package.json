{
  "name": "react-moviedb-store",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "bootswatch": "^4.5.0",
    "dotenv": "^8.2.0",
    "framer-motion": "^2.6.6",
    "history": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spinners": "^0.9.0"
  },
  "scripts": {
    "start": "run-p start-app json-server",
    "start-app": "cross-env REACT_APP_API_BASE_URL=http://localhost:5002/ react-scripts start",
    "json-server": "json-server --port 5002 --watch db.json",
    "format": "prettier \"src/**/*.{js,jsx,html}\" --write",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-console": "warn"
